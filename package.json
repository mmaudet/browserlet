    "jsdom": "^27.4.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "wxt": "^0.20.13"
  },
  "dependencies": {
    "@json2csv/plainjs": "^7.0.6",
    "file-saver": "^2.0.5",
    "js-yaml": "^4.1.1",
    "monaco-editor": "^0.55.1",
    "monaco-yaml": "^5.4.0",
    "vanjs-core": "^1.6.0",
    "vite-plugin-monaco-editor": "^1.1.0"
  }
